      <Label fx:id="knowledgeAcademy" />
      <Label text="QA:" />
      <Label fx:id="qicAcademy" />
    </HBox>
    <HBox prefHeight="25" prefWidth="300" spacing="7">
      <Label text="Trading Posts: " />
      <Label fx:id="tradingPosts" />
      <Label text="Research Labs: " />
      <Label fx:id="researchLabs" />
    </HBox>
    <HBox prefHeight="25" prefWidth="300" spacing="7">
      <Label text="Mines: " />
      <Label fx:id="mines" />
      <Label text="Gaiaformers: " />
      <Label fx:id="gaiaformers" />
    </HBox>
  </children>
</fx:root>
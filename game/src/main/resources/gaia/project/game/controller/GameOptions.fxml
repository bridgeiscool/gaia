<?xml version='1.0' encoding='UTF-8'?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<?import javafx.event.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.collections.*?>

<VBox styleClass="vbox-center" xmlns:fx="http://javafx.com/fxml">
  <stylesheets>
    <URL value="@ohellApp.css" />
  </stylesheets>
  <GridPane alignment="center" hgap="10" vgap="10">
    <Label text="Number of Players" GridPane.rowIndex="0" GridPane.columnIndex="0" />
    <ComboBox fx:id="numPlayers" GridPane.rowIndex="0" GridPane.columnIndex="1">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <Integer fx:value="3" />
        </FXCollections>
      </items>
    </ComboBox>
    <Label text="Race Selection" GridPane.rowIndex="1" GridPane.columnIndex="0" />
    <ComboBox fx:id="setup" GridPane.rowIndex="1" GridPane.columnIndex="1" />
    <Label text="Original Board" GridPane.rowIndex="2" GridPane.columnIndex="0" />
    <CheckBox fx:id="originalBoard" GridPane.rowIndex="2" GridPane.columnIndex="1" />
    <Label text="Buff Baltaks PI" GridPane.rowIndex="3" GridPane.columnIndex="0" />
    <CheckBox fx:id="baltaksBuff" GridPane.rowIndex="3" GridPane.columnIndex="1" />
    <Label text="RNG Seed" GridPane.rowIndex="4" GridPane.columnIndex="0" />
    <TextField fx:id="seed" text="0" GridPane.rowIndex="4" GridPane.columnIndex="1" />
  </GridPane>
</VBox>